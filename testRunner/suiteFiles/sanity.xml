<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Amazon_Sanity_Suite" parallel="tests" thread-count="1">
	<groups>
		<run>
			<include name="sanity"></include>
		</run>
	</groups>

	<listeners>
		<listener class-name="com.amazon.customlisteners.MyListener"></listener>
	</listeners>

	<test name="Amazon_SigninPage_Sanity_Test">
		<classes>
			<class name="com.amazon.tests.SigninPageTest" />
		</classes>
	</test>
	<test name="Amazon_RegistrationPageTest_Sanity_Test">
		<classes>
			<class name="com.amazon.tests.RegistrationPageTest" />
		</classes>
	</test>
</suite> 
